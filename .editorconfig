# http://editorconfig.org

root = true

[*]
charset = utf-8
indent_style = tab
indent_size = 2
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = true

# Windows specific files
[*.bat]
charset = latin1
end_of_line = crlf

# Space indented files
[*.py]
indent_style = space
indent_size = 2
